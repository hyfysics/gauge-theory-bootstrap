Cell[TextData[{
 "The sections \[OpenCurlyDoubleQuote]Kernels for physical region s>4\
\[CloseCurlyDoubleQuote] and \[OpenCurlyDoubleQuote]Kernels for unphysical \
region 0<s<4\[CloseCurlyDoubleQuote] give integration kernels ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalA]", "(", 
     RowBox[{"s", ",", "x"}], ")"}], ",", " ", 
    RowBox[{"\[ScriptCapitalW]", "(", 
     RowBox[{"s", ",", "y", ",", "x"}], ")"}], ",", " ", 
    RowBox[{
     SubscriptBox["\[ScriptCapitalW]", "00"], "(", 
     RowBox[{"x", ",", "y"}], ")"}], ",", " ", 
    RowBox[{
     SubscriptBox["\[ScriptCapitalW]", "0"], "(", "x", ")"}], ",", " ", 
    RowBox[{"\[ScriptCapitalK]", "(", 
     RowBox[{"s", ",", "x"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "cf38e2d3-8180-4181-9721-43531f55f362"],
 " (eq. (8.6)) in the contexts of physical region computations ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"s", ">", "4"}], ")"}], TraditionalForm]],ExpressionUUID->
  "0ba9a237-e8c7-41c6-92b8-05296f4ac4db"],
 " and unphysical region computations ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"0", "<", "s", "<", "4"}], ")"}], TraditionalForm]],
  ExpressionUUID->"e12bbaf1-c6c7-4ccf-9c0a-3b8a088eecf5"],
 ". "
}], "Text",
